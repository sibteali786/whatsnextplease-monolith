{
  "name": "web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && pnpm run build:cdk && prisma migrate deploy && next build",
    "build:cdk": "npm --prefix cdk run build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --noEmit",
    "db:studio": "prisma studio",
    "db:delete-file": "tsx deleteFile.ts",
    "db:deleteData": "tsx deleteData.ts",
    "dev:setup": "pnpm run db:up && pnpm run db:ready && pnpm run db:migrate && pnpm run db:seed && pnpm run dev",
    "db:up": "docker-compose up -d",
    "db:ready": "bash ./scripts/wait-for-db.sh",
    "db:migrate": "prisma migrate dev",
    "db:seed": "npm run db:seed:script",
    "db:seed:script": "tsx seed.ts",
    "db:seed:placeholder": "tsx placeholder-seed.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
	"db:migrate:dev": "prisma migrate dev",
	"db:migrate:deploy": "prisma migrate deploy"
  },
